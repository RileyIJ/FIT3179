{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "config": {"view": {"stroke": ""}},
  "width": 200,
  "height": 20,
  "data": {
    "values": [
      {"row": "1", "image": "ship"},
      {"row": "1", "image": "ship"},
      {"row": "1", "image": "ship"},
      {"row": "1", "image": "ship"},
      {"row": "1", "image": "ship"},
      {"row": "2", "image": "spacer"},
      {"row": "2", "image": "ship"},
      {"row": "2", "image": "ship"},
      {"row": "2", "image": "ship"}
    ]
  },
  "transform": [
    {
      "calculate": "{'ship': 'ðŸš¢', 'spacer': ' '}[datum.image]",
      "as": "emoji"
    },
    {"window": [{"op": "rank", "as": "rank"}], "groupby": ["row"]}
  ],
  "mark": {"type": "text", "baseline": "middle"},
  "encoding": {
    "x": {"field": "rank", "type": "ordinal", "axis": null},
    "y": {"field": "animal", "type": "nominal", "axis": null, "sort": null},
    "row": {"field": "row", "header": {"title": ""}},
    "text": {"field": "emoji", "type": "nominal"},
    "size": {"value": 30}
  }
}